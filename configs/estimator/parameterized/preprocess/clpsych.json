{
    "output":"./data/results/estimator/datasets/clpsych/",
    "inputs":[
        "labeled-clpsych"
    ],
    "inputs_metadata":{
        "platform":"twitter",
        "user_per_file": true
    },

    "cache_user_criteria":{
        "date_boundaries":[
            {"min_date":"2012-01-01", "max_date":"2014-01-01"}
        ],
        "min_posts_per_date_boundary":200,
        "intersection":false,
        "lang":["en"],
        "include_retweets":true
    },
    "cache_kwargs":{
        "chunksize":5000,
        "within_chunk_chunksize":10000,
        "date_boundaries":{
            "min_date":"2012-01-01",
            "max_date":"2014-01-01"
        },
        "sample_rate":null
    },

    "vectorize_kwargs":{
        "min_n_posts":200,
        "n_posts":null,
        "date_boundaries":[
            {"min_date":"2012-01-01", "max_date":"2014-01-01"}
        ],
        "vocab_kwargs": {
            "preserve_case": false,
            "filter_negate": true,
            "filter_upper": true,
            "filter_punctuation": true,
            "filter_numeric": true,
            "filter_user_mentions": true,
            "filter_url": true,
            "filter_retweet": true,
            "filter_stopwords": false,
            "keep_pronouns": true,
            "emoji_handling": null,
            "filter_hashtag":false,
            "strip_hashtag":false,
            "filter_mh_subreddits":"all",
            "filter_mh_terms":"smhd",
            "max_tokens_per_document": null,
            "max_documents_per_user": null
        },
        "phrasers":null,
        "vocabulary":[]
    },

    "random_state":42
}